emon.push(null)\n                }\n            }\n        } else if (team.pokemon.length === 6) {\n            \n        } else {\n            fullPokemon = team.pokemon.slice(0, 6)\n        }\n\n        updateTeam({\n            ...team,\n            pokemon: fullPokemon\n        })\n    }, [team])\n\n    const renderTeamSlot = (pokemon, index) => {\n\n        const handlePokemonJoin = (payload) => {\n            const {pokemon} = payload\n            let pokemonTeam = [...team.pokemon]\n            pokemonTeam[index] = pokemon\n            updateTeam({\n                ...team,\n                pokemon: pokemonTeam,\n            })\n        }\n\n        const handlePokemonLeft = () => {\n            let pokemonTeam = [...team.pokemon]\n            pokemonTeam[index] = null\n            updateTeam({\n                ...team,\n                pokemon: pokemonTeam,\n            })\n        }\n\n        return <TeamSlot key={index} pokemon={pokemon} onJoin={handlePokemonJoin} onLeave={handlePokemonLeft} />\n    }\n\n    return (\n        <div>\n            <h2>Team Name</h2>\n            <ul className=\"tiles\">\n                {team.pokemon.map(renderTeamSlot)}\n            </ul>\n        </div>\n    )\n}","/Users/admin/Development/code/mod-5/poke-time-project/PokeTime-FrontEnd/poke-time/src/components/Pokemon.js",[],"/Users/admin/Development/code/mod-5/poke-time-project/PokeTime-FrontEnd/poke-time/src/components/TwitterContainer.js",[],"/Users/admin/Development/code/mod-5/poke-time-project/PokeTime-FrontEnd/poke-time/src/components/DraggablePokemon.js",["103"],"import React from \"react\";\nimport { useDrag } from \"react-dnd\";\nimport { ItemTypes } from \"./Constants\"\nimport Pokemon from \"./Pokemon\"\n\nexport default function DraggablePokemon(props) {\n    const {pokemon, onPickup, onDrop} = props\n\n    const [_dndProps, drag] = useDrag({\n        item: { \n            type: ItemTypes.POKEMON,\n            pokemon: props.pokemon,\n        },\n        begin: