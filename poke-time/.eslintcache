 exact path=\"/\" component={Home} />\n            <Route path=\"/profile\" component={ProfileContainer} />\n            <Route path=\"/friends\" component={FriendsContainer} />\n            <Route path=\"/SignOut\" component={SignOut} />\n          </Switch>\n        </React.Fragment>\n      ) : (\n        <>\n          <Header />\n          <Switch>\n            <Route exact path=\"/\" component={Home} />\n            <Route path=\"/login\" component={LoginContainer} />\n            <Route path=\"/signup\" component={SignupContainer} />\n          </Switch>\n        </>\n      )}\n      {/* <button onClick={handleAuthClick} className=\"ui button\">Access Authorized Route</button> */}\n    </div>\n  );\n}\n\nexport default App;\n","/Users/admin/Development/code/mod-5/poke-time-project/PokeTime-FrontEnd/poke-time/src/components/Header.js",[],"/Users/admin/Development/code/mod-5/poke-time-project/PokeTime-FrontEnd/poke-time/src/components/LoginForm.js",[],"/Users/admin/Development/code/mod-5/poke-time-project/PokeTime-FrontEnd/poke-time/src/components/SignInForm.js",[],"/Users/admin/Development/code/mod-5/poke-time-project/PokeTime-FrontEnd/poke-time/src/reportWebVitals.js",[],"/Users/admin/Development/code/mod-5/poke-time-project/PokeTime-FrontEnd/poke-time/src/components/PokemonList.js",["58"],"/Users/admin/Development/code/mod-5/poke-time-project/PokeTime-FrontEnd/poke-time/src/components/FriendsContainer.js",[],"/Users/admin/Development/code/mod-5/poke-time-project/PokeTime-FrontEnd/poke-time/src/components/SignOut.js",[],"/Users/admin/Development/code/mod-5/poke-time-project/PokeTime-FrontEnd/poke-time/src/components/Home.js",[],"/Users/admin/Development/code/mod-5/poke-time-project/PokeTime-FrontEnd/poke-time/src/components/Profile.js",[],"/Users/admin/Development/code/mod-5/poke-time-project/PokeTime-FrontEnd/poke-time/src/components/NavBar.js",[],{"ruleId":"59","replacedBy":"60"},{"ruleId":"61","replacedBy":"62"},{"ruleId":"63","severity":1,"message":"64","li