setPokemonData(data))\n  }\n\n  const LoginContainer = () => {\n    return <LoginForm handleLogin={handleLogin}/>\n  }\n\n  const SignupContainer = () => {\n    return <SignInForm handleLogin={handleLogin}/>\n  }\n\n  const ProfileContainer = () => {\n    if (!user) return null;\n    if (!pokemonData) return null;\n\n    return <Profile pokemon={pokemonData} user={user} />\n  }\n\n  const TeamProfileContainer = () => {\n    if(!user) return null;\n    if(!pokemonData) return null;\n\n    return <TeamProfile pokemon={pokemonData} teams={user.teams} />\n  }\n\n  const clearUser = () => {\n    setUser(null)\n  }\n\n  return (\n    <div className=\"App\">\n      {user ? (\n        <React.Fragment>\n          <Header user={user} clearUser={clearUser} />\n          <Switch>\n            <Route exact path=\"/\" component={Home} />\n            <Route path=\"/profile\" component={ProfileContainer} />\n            <Route path=\"/friends\" component={FriendsContainer} />\n            <Route path=\"/team_lists\" component={TeamProfileContainer} />\n          </Switch>\n        </React.Fragment>\n      ) : (\n        <>\n          <Header />\n          <Switch>\n            <Route exact path=\"/\" component={Home} />\n            <Route path=\"/login\" component={LoginContainer} />\n            <Route path=\"/signup\" component={SignupContainer} />\n          </Switch>\n        </>\n      )}\n      {/* <button onClick={handleAuthClick} className=\"ui button\">Access Authorized Route</button> */}\n    </div>\n  );\n}\n\nexport default App;\n","/Users/admin/Development/code/mod-5/poke-time-project/PokeTime-FrontEnd/poke-time/src/components/Header.js",[],"/Users/admin/Development/code/mod-5/poke-time-project/PokeTime-FrontEnd/poke-time/src/components/LoginForm.js",[],"/Users/admin/Development/code/mod-5/poke-time-project/PokeTime-FrontEnd/poke-time/src/components/SignInForm.js",[],"/Users/admin/Development/code/mod-5/poke-time-project/PokeTime-FrontEnd/poke-time/src/reportWebVital